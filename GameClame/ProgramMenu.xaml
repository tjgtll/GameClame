<Window x:Class="GameClame.ProgramMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameClame"
        mc:Ignorable="d"
        Title="ProgramMenu" Height="450" Width="800"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="FlappyBird" Content="Flappy Bird" HorizontalAlignment="Center" Margin="0,202,0,0" VerticalAlignment="Top" Width="155" Click="FlappyBird_Click"/>
        <Button x:Name="Tetris" Content="Tetris" HorizontalAlignment="Center" Margin="0,243,0,0" VerticalAlignment="Top" Width="156" Click="Tetris_Click"/>
        <Button x:Name="TicTac" Content="Tic-Tac" HorizontalAlignment="Center" Margin="0,282,0,0" VerticalAlignment="Top" Width="156" Click="TicTac_Click"/>

    </Grid>
</Window>
